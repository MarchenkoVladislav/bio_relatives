<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <File name="LogFile">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </File>
    </Appenders>
    <Loggers>
        <Logger name= "bio_relatives_logger" level="debug" additivity="true">
            <AppenderRef ref="LogFile"/>
        </Logger>
        <Root level="all">
            <AppenderRef ref="LogFile"/>
        </Root>
    </Loggers>
</Configuration>